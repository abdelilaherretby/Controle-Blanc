<VBox alignment="CENTER" prefHeight="515.0" prefWidth="685.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="org.example.controle_blanc.Controller.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="551.0" prefWidth="643.0">
         <children>
            <Label layoutX="165.0" layoutY="108.0" prefHeight="31.0" prefWidth="327.0" text="Ajouter un nouveau membre ">
               <font>
                  <Font name="Arial" size="25.0" />
               </font>
            </Label>
            <TextField fx:id="fnom" layoutX="202.0" layoutY="166.0" prefHeight="35.0" prefWidth="253.0" promptText="Nom" />
            <TextField fx:id="fprenom" layoutX="202.0" layoutY="219.0" prefHeight="35.0" prefWidth="253.0" promptText="Prenom" />
            <TextField fx:id="femail" layoutX="202.0" layoutY="276.0" prefHeight="35.0" prefWidth="253.0" promptText="Email" />
            <TextField fx:id="fphone" layoutX="202.0" layoutY="333.0" prefHeight="35.0" prefWidth="253.0" promptText="Phone" />
            <Button fx:id="ajout" layoutX="265.0" layoutY="396.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="30.0" prefWidth="108.0" text="Inserer" />
         </children>
      </AnchorPane>
   </children>
</VBox>
